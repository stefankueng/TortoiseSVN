#. (title)
#. (secondary)
msgid "TortoiseSVN"
msgstr "TortoiseSVN"

#. (subtitle)
msgid "A Subversion client for Windows"
msgstr "针对 Windows 平台的 Subversion 客户端"

#. (edition)
msgid "First"
msgstr "首先"

#. (firstname)
msgid "Stefan"
msgstr "Stefan"

#. (surname)
msgid "Küng"
msgstr "Küng"

#. (firstname)
msgid "Lübbe"
msgstr "Lübbe"

