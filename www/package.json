{
  "name": "TortoiseSVN-web",
  "version": "0.2.0",
  "author": "XhmikosR (https://github.com/XhmikosR)",
  "description": "The dependencies to build TortoiseSVN's website",
  "homepage": "http://tortoisesvn.net/",
  "repository": {
    "type": "svn",
    "url": "http://tortoisesvn.googlecode.com/svn/trunk/"
  },
  "bugs": {
    "url": "http://tortoisesvn.net/reportbug.html"
  },
  "license": "GPLv2",
  "scripts": {
    "lint": "node run-tests"
  },
  "dependencies": {
    "clean-css": "~2.1.0",
    "csslint": "~0.10.0",
    "jshint": "~2.4.3",
    "shelljs": "~0.2.6",
    "uglify-js": "~2.4.12"
  },
  "jshintConfig": {
    "bitwise": true,
    "boss": true,
    "browser" : false,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "indent": 4,
    "latedef": true,
    "multistr": true,
    "noarg": true,
    "node" : true,
    "noempty": true,
    "plusplus": false,
    "quotmark": "double",
    "shelljs": true,
    "strict": false,
    "trailing": true,
    "unused": true
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
