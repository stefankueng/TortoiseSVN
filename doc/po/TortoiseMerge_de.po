#: (term)
msgid "/mine:\"path/to/yourfile\""
msgstr "/mine:\"path/to/yourfile\""

#: source/TortoiseMerge_en.xml:908
#: (para)
msgid "load your file"
msgstr "Lädt die eigene Datei"

#: source/TortoiseMerge_en.xml:914
#: (term)
msgid "/minename:name"
msgstr "/minename:name"

#: source/TortoiseMerge_en.xml:916
#: (para)
