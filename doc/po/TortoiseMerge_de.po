msgstr "Setzt den Titel für die <quote>meiner</quote> Datei."

#: source/TortoiseMerge_en.xml:922
#: (term)
msgid "/merged:\"path/to/mergedfile\""
msgstr "/merged:\"path/to/mergedfile\""

#: source/TortoiseMerge_en.xml:924
#: (para)
msgid "Specifies the merged file where the result of the merge is saved to."
msgstr "Gibt die Zieldatei an, in der das Ergebnis gespeichert wird."

#: source/TortoiseMerge_en.xml:931
#: (term)
msgid "/mergedname:name"
msgstr "/mergedname:name"

#: source/TortoiseMerge_en.xml:933
#: (para)
msgid "Sets the view title for the result file."
msgstr "Setzt den Titel für die Ergebnisdatei."

#: source/TortoiseMerge_en.xml:939
#: (term)
msgid "/diff:\"path/to/unified_diff_file\""
msgstr "/diff:\"path/to/unified_diff_file\""

#: source/TortoiseMerge_en.xml:941
#: (para)
