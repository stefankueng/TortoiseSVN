msgid "If you add a file or folder by mistake, you can undo the addition before you commit using <menuchoice><guimenu>TortoiseSVN</guimenu><guimenuitem>Undo add...</guimenuitem></menuchoice>."
msgstr "Keď chcete vidieť zmeny, ktoré ste <emphasis>vy</emphasis>urobili v pracovnej kópií, jednoducho vyberte kontextové menu<menuchoice><guimenu>TortoiseSVN</guimenu><guimenuitem>Porovnať</guimenuitem></menuchoice>."

#. (title)
#: ../source/TortoiseSVN_en.xml:6677
#, fuzzy
msgid "Copying/Moving/Renaming Files and Folders"
msgstr "Presúvanie súborov a adresárov"

#. (primary)
