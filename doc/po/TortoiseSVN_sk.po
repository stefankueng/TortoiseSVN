#: ../source/TortoiseSVN_en.xml:7633
#, fuzzy
msgid "TortoiseSVN Project Properties"
msgstr "TortoiseSVN Vlastnosti"

#. (primary)
#: ../source/TortoiseSVN_en.xml:7635
#, fuzzy
msgid "project properties"
msgstr "Vlastnosť projektu"

