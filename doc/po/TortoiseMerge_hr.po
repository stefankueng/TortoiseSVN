msgstr "Kopiraj označene redke u priručni spremnik"

#: source/TortoiseMerge_en.xml:801
#: (term)
msgid "Ctrl-F"
msgstr "Ctrl-F"

#: source/TortoiseMerge_en.xml:803
#: (para)
msgid "Open the Find dialog for text searches"
msgstr "Otvara dijalog za pretraživanje teksta"

#: source/TortoiseMerge_en.xml:809
#: (term)
msgid "Ctrl-O"
msgstr "Ctrl-O"

#: source/TortoiseMerge_en.xml:811
#: (para)
msgid "Open files to diff/merge"
msgstr "Otvara datoteke za usporedbu/stapanje"

#: source/TortoiseMerge_en.xml:817
#: (term)
msgid "Ctrl-S"
msgstr "Ctrl-S"

#: source/TortoiseMerge_en.xml:819
#: (para)
msgid "Save the changes"
msgstr "Snima promjene"

#: source/TortoiseMerge_en.xml:825
#: (term)
msgid "Ctrl-Shift-S"
msgstr "Ctrl-Shift-S"

#: source/TortoiseMerge_en.xml:827
