<?xml version="1.0" encoding="UTF-8"?>
<fop version="1.0">
<!--<!DOCTYPE configuration SYSTEM "config.dtd">-->
<!--
  this file contains templates which allow an user easy
  configuration of Fop. Actually normally you don't need this configuration
  file, but if you need to change configuration, you should
  always use this file and *not* config.xml.
  Usage: java org.apache.fop.apps.Fop -c userconfig.xml -fo fo-file -pdf pdf-file
-->

<!--
  this file is translated automatically to userconfig.xml by our
  build script.
-->

  <source-resolution>96</source-resolution>
  <target-resolution>96</target-resolution>

<!--
************************************************************************
  Add fonts here. The %WINFONTSDIR% string is replaced with your system
  fonts directory. Always use %WINFONTSDIR% to avoid fixed paths which
  might not work on other systems than yours.
  the %USRFONTSDIR% is replaced with the path to the kerning files
************************************************************************
-->
<renderers>
  <renderer mime="application/pdf">
  <fonts>
    <font metrics-url="%USRFONTSDIR%/arial.xml" kerning="yes" embed-url="%WINFONTSDIR%/arial.ttf">
      <font-triplet name="Arial" style="normal" weight="normal"/>
      <font-triplet name="ArialMT" style="normal" weight="normal"/>
      <font-triplet name="Helvetica" style="normal" weight="normal"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/arialbd.xml" kerning="yes" embed-url="%WINFONTSDIR%/arialbd.ttf">
      <font-triplet name="Arial" style="normal" weight="bold"/>
      <font-triplet name="ArialMT" style="normal" weight="bold"/>
      <font-triplet name="Helvetica" style="normal" weight="bold"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/ariali.xml" kerning="yes" embed-url="%WINFONTSDIR%/ariali.ttf">
      <font-triplet name="Arial" style="italic" weight="normal"/>
      <font-triplet name="ArialMT" style="italic" weight="normal"/>
      <font-triplet name="Helvetica" style="italic" weight="normal"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/arialbi.xml" kerning="yes" embed-url="%WINFONTSDIR%/arialbi.ttf">
      <font-triplet name="Arial" style="italic" weight="bold"/>
      <font-triplet name="ArialMT" style="italic" weight="bold"/>
      <font-triplet name="Helvetica" style="italic" weight="bold"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/times.xml" kerning="yes" embed-url="%WINFONTSDIR%/times.ttf">
      <font-triplet name="Times" style="normal" weight="normal"/>
      <font-triplet name="TimesMT" style="normal" weight="normal"/>
      <font-triplet name="Times New Roman" style="normal" weight="normal"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/timesbd.xml" kerning="yes" embed-url="%WINFONTSDIR%/timesbd.ttf">
      <font-triplet name="Times" style="normal" weight="bold"/>
      <font-triplet name="TimesMT" style="normal" weight="bold"/>
      <font-triplet name="Times New Roman" style="normal" weight="bold"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/timesi.xml" kerning="yes" embed-url="%WINFONTSDIR%/timesi.ttf">
      <font-triplet name="Times" style="italic" weight="normal"/>
      <font-triplet name="TimesMT" style="italic" weight="normal"/>
      <font-triplet name="Times New Roman" style="italic" weight="normal"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/timesbi.xml" kerning="yes" embed-url="%WINFONTSDIR%/timesbi.ttf">
      <font-triplet name="Times" style="italic" weight="bold"/>
      <font-triplet name="TimesMT" style="italic" weight="bold"/>
      <font-triplet name="Times New Roman" style="italic" weight="bold"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/cour.xml" kerning="yes" embed-url="%WINFONTSDIR%/cour.ttf">
      <font-triplet name="Courier" style="normal" weight="normal"/>
      <font-triplet name="CourierMT" style="normal" weight="normal"/>
      <font-triplet name="Courier New" style="normal" weight="normal"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/courbd.xml" kerning="yes" embed-url="%WINFONTSDIR%/courbd.ttf">
      <font-triplet name="Courier" style="normal" weight="bold"/>
      <font-triplet name="CourierMT" style="normal" weight="bold"/>
      <font-triplet name="Courier New" style="normal" weight="bold"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/couri.xml" kerning="yes" embed-url="%WINFONTSDIR%/couri.ttf">
      <font-triplet name="Courier" style="italic" weight="normal"/>
      <font-triplet name="CourierMT" style="italic" weight="normal"/>
      <font-triplet name="Courier New" style="italic" weight="normal"/>
    </font>
    <font metrics-url="%USRFONTSDIR%/courbi.xml" kerning="yes" embed-url="%WINFONTSDIR%/courbi.ttf">
      <font-triplet name="Courier" style="italic" weight="bold"/>
      <font-triplet name="CourierMT" style="italic" weight="bold"/>
      <font-triplet name="Courier New" style="italic" weight="bold"/>
    </font>
  </fonts>
  </renderer>
</renderers>

</fop>
