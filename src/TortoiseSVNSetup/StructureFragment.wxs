          <Component Id="C__machineregistry" Guid="$(var.GuidMachineRegistry)" Win64="$(var.Win64YesNo)">
            <Registry Root="HKLM" Key="Software\[Manufacturer]" Action="createKeyAndRemoveKeyOnUninstall" />
            <Registry Root="HKLM" Key="Software\[Manufacturer]" Name="ReadOnlyIcon" Value="[INSTALLDIR]icons\XPStyle\TortoiseReadOnly.ico" Type="string" KeyPath="yes" />
            <Registry Root="HKLM" Key="Software\[Manufacturer]" Name="DeletedIcon" Value="[INSTALLDIR]icons\XPStyle\TortoiseDeleted.ico" Type="string" />
            <Registry Root="HKLM" Key="Software\[Manufacturer]" Name="ProcPath" Value="[INSTALLDIR]bin\TortoiseProc.exe" Type="string" />
            <Registry Root="HKLM" Key="Software\[Manufacturer]" Name="CachePath" Value="[INSTALLDIR]bin\TSVNCache.exe" Type="string" />
            <Registry Root="HKLM" Key="Software\[Manufacturer]" Name="InSubversionIcon" Value="[INSTALLDIR]icons\XPStyle\TortoiseInSubversion.ico" Type="string" />
            <Registry Root="HKLM" Key="Software\[Manufacturer]" Name="TMergePath" Value="[INSTALLDIR]bin\TortoiseMerge.exe" Type="string" />
            <Registry Root="HKLM" Key="Software\[Manufacturer]" Name="ModifiedIcon" Value="[INSTALLDIR]icons\XPStyle\TortoiseModified.ico" Type="string" />
            <Registry Root="HKLM" Key="Software\[Manufacturer]" Name="Directory" Value="[INSTALLDIR]" Type="string" />
            <Registry Root="HKLM" Key="Software\[Manufacturer]" Name="ConflictIcon" Value="[INSTALLDIR]icons\XPStyle\TortoiseConflict.ico" Type="string" />
